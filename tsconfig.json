{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"include": [
		"app-ui/src/**/*"
	],
	"exclude": [
		"node_modules/*",
		"app-ui/build/*"
	],
	"compilerOptions": {
		"strict": true,
		"types": [],
		"baseUrl": "./app-ui",
		"paths": {
			"@src/": ["src/"],
			"@src/*": ["src/*"],
			"@async-options/*": ["src/utils/AsyncOptions/*"]
		},
	},

}
